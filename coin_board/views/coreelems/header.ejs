<noscript>
	<div class="alert alert-danger text-center flex-row" role="alert">
		<h2><%= title %>
			work best with JavaScript enabled ;)</h2>
	</div>
</noscript>
<nav class="navbar navbar-dark flex-column flex-md-row fixed-top bg-primary">
	<a class="navbar-brand mr-0 mr-md-2" href="/">
		<span class="navbar-toggler-icon"></span>
		<span class="lnr lnr-diamond text-white">
			Coin_Board
		</span>
	</a>
	<% if (locals.data && locals.data.username) {%>
		<a class="navbar-text text-white" href="/profile">
			<span class="lnr lnr-user"></span>
			<%= locals.data.username %>
		</a>
	<%}%>
	<% if (!locals.data || !locals.data.username) {%>
		<a class="navbar-text text-white" href="/login">
			<span class="lnr lnr-power-switch"></span>
			Login
		</a>
		<a class="navbar-text text-white" href="/signin">
			<span class="lnr lnr-database"></span>
			Register
		</a>
	<%}%>
	<a class="navbar-text text-white" href="/assets/dashboard">
		<span class="lnr lnr-rocket"></span>
		Dashboard
	</a>
	<a class="navbar-text text-white" href="/assets/infofeed">
		<span class="lnr lnr-bullhorn"></span>
		News Feeds
	</a>
	<a class="navbar-text text-white" href="https://github.com/MyAssetBoard/coinboard-webapp">
		<span class="lnr lnr-code"></span>
		Github
	</a>
	<div class="float-right popUp hidden" id="popup">
		<div class="alert" id="ppContent"></div>
	</div>
