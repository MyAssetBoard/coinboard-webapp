<div class="card">
	<div class="card-header text-left">
		<h5>
			<li><%= id %></li>
		</h5>
	</div>
	<div class="card-body">
		<table class="table">
			<thead>
				<tr>
					<th scope="col">#ID</th>
					<th scope="col">Account</th>
					<th scope="col">Secret</th>
					<th scope="col"></th>
				</tr>
			</thead>
			<tbody>
				<tr>
					<th class="input-group" scope="row">1</th>
					<td>
						<%= key %>
					</td>
					<td>
						<%= secret %>
					</td>
					<td>
						<a class="btn btn-primary
						btn-sm text-white">
							Update
						</a>
					</td>
				</tr>
			</tbody>
		</table>
	</div>
</div>