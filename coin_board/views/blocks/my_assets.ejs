<div class="card hidden"
    id="yoloshowAdd">
	<div class="card-block">
		<div class="card-header">
			<ul class="nav nav-pills nav-fill">
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#Fiat">Fiat Accounts </a> </li>
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#Eth">Ethereum</a> </li>
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#EthToken">ERC20 Tokens</a> </li>
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#Neo">NEO</a> </li>
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#NeoToken">NEP5-6 Tokens</a> </li>
				<li class="nav-item"> <a class="nav-link"
					    data-toggle="tab"
					    href="#Shitcoin">ALTSCoin</a> </li>
			</ul>
		</div>
		<div class="tab-content card-body">
			<div id="Fiat"
			    class="tab-pane fade show active visible">
				<form>
					<div class="form-row">
						<div class="col"> <select class="form-control"
							    id="Fiatcurr">
								<option>EUR</option>
								<option>USD</option>
								</select> </div>
						<div class="col"> <input id="Zqtt"
							    type="text"
							    class="form-control form-control-sm"
							    placeholder="enter amount"> </div>
					</div>
				</form>
			</div>
			<div id="Eth"
			    class="tab-pane fade">
				<div class="form-row">
					<div class="col"> <button id="Zticker"
						    class="btn btn-secondary btn-sm btn-block"
						    value="ETH">+ETH</button> </div>
					<div class="col"> <input id="Zqtt"
						    type="text"
						    class="form-control form-control-sm"
						    placeholder="enter amount"> </div>
				</div>
			</div>
			<div id="EthToken"
			    class="tab-pane fade">
				<div class="form-row">
					<div class="col"> <button id="Zticker"
						    type="button"
						    class="btn btn-secondary btn-sm btn-block"
						    value="ERC20">+ERC20</button> </div>
					<div class="col"> <input id="Zqtt"
						    type="text"
						    class="form-control form-control-sm"
						    placeholder="enter amount"> </div>
				</div>
			</div>
			<div id="Neo"
			    class="tab-pane fade">
				<div class="form-row">
					<div class="col"> <button id="Zticker"
						    type="button"
						    class="btn btn-secondary btn-sm btn-block"
						    value="NEO">+NEO</button> </div>
					<div class="col"> <input id="Zqtt"
						    type="text"
						    class="form-control form-control-sm"
						    placeholder="enter amount"> </div>
				</div>
			</div>
			<div id="NeoToken"
			    class="tab-pane fade">
				<div class="form-row">
					<div class="col"> <button id="Zticker"
						    type="button"
						    class="btn btn-secondary btn-sm btn-block"
						    value="NEP5-6">+NEP5-6</button> </div>
					<div class="col"> <input id="Zqtt"
						    type="text"
						    class="form-control form-control-sm"
						    placeholder="enter amount"> </div>
				</div>
			</div>
			<div id="Shitcoin"
			    class="tab-pane fade"> </div>
		</div>
	</div>
</div>
<div id="accordion">
	<div class="card">
		<div class="card-header"
		    id="headingOne">
			<h5 class="mb-0">
        <button class="btn btn-secondary btn-lg btn-block dropdown-toggle"
	data-toggle="collapse" data-target="#collapseOne"
	aria-expanded="true" aria-controls="collapseOne">
          <span class="lnr lnr-briefcase"></span> My assets
        </button>
      </h5> </div>
		<div id="collapseOne"
		    class="collapse show"
		    aria-labelledby="headingOne"
		    data-parent="#accordion">
			<div class="card-body">
				<% if (locals.data && locals.data.username) {%>
					<%= locals.data.username %>
						<%} else {
				 %>Please <a href="/login">LOGIN </a>
							<%}%>
			</div>
		</div>
	</div>
	<div class="card">
		<div class="card-header"
		    id="headingTwo">
			<h5 class="mb-0">
        <button class="btn btn-secondary btn-lg btn-block dropdown-toggle collapsed" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="false" aria-controls="collapseTwo">
          [+] Add
        </button>
      </h5> </div>
		<div id="collapseTwo"
		    class="collapse"
		    aria-labelledby="headingTwo"
		    data-parent="#accordion">
			<div class="card-body"><select type="select"
				    id="ticker"
				    class="form-control">
			<% if (locals.symb && Object.keys(symb).length) {%>
				<% for (var elems in symb){ %>
					<option value="<%- symb[elems] %>">
					<%=symb[elems]%></option>
					 <%} %>
				<%}
			%>
			</select> </div>
			<div class="card-body"><input id="qtt"
				    type="text"
				    class="form-control"
				    placeholder="enter amount"> </div>
			<div class="card-body"> <button id="add"
				    class="btn btn-lg btn-block btn-info"> OK
			        </button> </div>
		</div>
		<div class="card-footer text-muted">Last update : 2 days ago </div>
	</div>
</div>
</div>