{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "exclude": ["node_modules", "coin_board/node_modules"],
    "include": ["coin_board/", "coin_board/bin"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    }
  }
}
